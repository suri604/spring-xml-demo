<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean id="actor" class="com.stackroute.domain.Actor">
        <constructor-arg name="Name" value="Akshay Kumar" ></constructor-arg>
        <constructor-arg name="Gender" value="Male"></constructor-arg>
        <constructor-arg name="Age" value="50"></constructor-arg>
    </bean>


    <bean id="movie" class ="com.stackroute.domain.Movie" autowire="constructor" >
    </bean>
    <bean class="com.stackroute.domain.BeanLifecycleDemoBean" init-method="customInit" destroy-method="customDestroy">
    </bean>



</beans>